# src/OMakefile

.PHONY: all clean submit

USE_OCAMLFIND = true

FILES[] =
	option
	nel
	extList
	extStr
	extSet
	common
	entities
	main


LIB = target
PROGRAM = a.out
# OCAML_LIBS +=
# OCAML_CLIBS +=
# OCAML_OTHER_LIBS +=
# OCAML_LIB_FLAGS +=
#
.DEFAULT: $(OCamlProgram $(PROGRAM), $(FILES))

OCamlLibrary($(LIB), $(FILES))


clean:
	find . -name "*.cm[ioxat]*" -o -name "*.out" -o -name "*.opt" -o -name "*.[ao]" \
	| xargs $(RM)

submit:
	./make-submit-file.py
	pbcopy < submit.ml
